{
    "expo": {
        "name": "El Impostor",
        "slug": "el-impostor",
        "version": "1.0.1",
        "orientation": "portrait",
        "icon": "./assets/icono-principal.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/icono-principal.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "scheme": "el-impostor",
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.unai_murillo.elimpostor",
            "infoPlist": {
                "CFBundleURLTypes": [
                    {
                        "CFBundleURLSchemes": [
                            "el-impostor"
                        ]
                    }
                ]
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icono-principal.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false,
            "package": "com.unai_murillo.elimpostor",
            "intentFilters": [
                {
                    "action": "VIEW",
                    "autoVerify": true,
                    "data": [
                        {
                            "scheme": "file"
                        },
                        {
                            "mimeType": "*/*"
                        },
                        {
                            "pathPattern": ".*\\.impostor"
                        }
                    ],
                    "category": [
                        "BROWSABLE",
                        "DEFAULT"
                    ]
                },
                {
                    "action": "VIEW",
                    "autoVerify": true,
                    "data": [
                        {
                            "scheme": "content"
                        },
                        {
                            "mimeType": "*/*"
                        },
                        {
                            "pathPattern": ".*\\.impostor"
                        }
                    ],
                    "category": [
                        "BROWSABLE",
                        "DEFAULT"
                    ]
                }
            ]
        },
        "web": {
            "favicon": "./assets/icono-principal.png"
        },
        "extra": {
            "eas": {
                "projectId": "2a7e644e-d015-408d-a8b3-f872477e070a"
            }
        }
    }
}